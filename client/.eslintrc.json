{
  "extends": [
    "airbnb",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, {"extensions": [".js"]}],
    "comma-dangle": [
      2,
      "only-multiline"
    ],
    "prettier/prettier": ["error", { "singleQuote": true }]
  }
}
